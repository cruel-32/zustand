{
    "singleQuote": true,
    "parser": "typescript",
    "semi": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "printWidth": 120,
    "arrowParens": "always",
    "plugins": [
        "prettier-plugin-tailwindcss"
    ],
    "tailwindConfig": "./tailwind.config.js",
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "strict",
    "endOfLine": "auto"
}

